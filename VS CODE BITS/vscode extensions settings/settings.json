// Place your settings in this file to overwrite the default settings
{
    "files.associations": {
        ".luacheckrc": "lua",
        "config.ld": "lua",
        "*.uiflow": "uiflow"
    },
    "code-runner.fileDirectoryAsCwd": false,
    "code-runner.runInTerminal": false,
    "code-runner.clearPreviousOutput": true,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    },
    "window.title": "${dirty}${rootName}${separator}${activeEditorShort}",
    "editor.formatOnPaste": false,
    "editor.formatOnType": true,
    "git.autofetch": true,
    "breadcrumbs.enabled": true,
    "breadcrumbs.filePath": "on",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.build": true,
        "**/.history": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true
    },
    "files.trimTrailingWhitespace": true,
    "search.followSymlinks": true,
    "problems.decorations.enabled": true,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.tabCompletion": "on",
    "todo-tree.regex": "((\\-\\-)\\s*(TODO|todo|Todo|FIXME)+|(\\-\\-\\s*!))",
    "todo-tree.showInExplorer": false,
    "todo-tree.defaultHighlight": {
        "foreground": "green",
        "type": "none"
    },
    "todo-tree.customHighlight": {
        "TODO": {},
        "FIXME": {}
    },
    "workbench.startupEditor": "newUntitledFile",
    "editor.wordWrap": "on",
    "editor.detectIndentation": false,
    "editor.formatOnSave": true,
    "editor.useTabStops": false,
    "files.trimFinalNewlines": true,
    "diffEditor.renderSideBySide": true,
    "gitlens.hovers.currentLine.over": "line",
    "editor.semanticHighlighting.enabled": true,
    "Lua.telemetry.enable": false,
    "Lua.diagnostics.disable": [
        "different-requires"
    ],
    "Lua.workspace.preloadFileSize": 999999999,
    "gitlens.hovers.currentLine.details": false,
    "gitlens.hovers.currentLine.changes": false,
    "gitlens.hovers.currentLine.enabled": false,
    "Lua.runtime.plugin": "../.vscode/lua/plugin.lua",
    "Lua.hover.enumsLimit": 10,
    "[lua]": {
        "editor.defaultFormatter": "sumneko.lua"
    },
    "vscode-lua-format.configPath": "E:\\Home\\Projects\\Factorio Modding\\.lua-format",
    "Lua.completion.callSnippet": "Replace",
    "Lua.diagnostics.workspaceDelay": 1000,
    "Lua.hover.fieldInfer": 5000,
    "Lua.intelliSense.searchDepth": 5,
    "Lua.completion.autoRequire": false,
    "security.workspace.trust.untrustedFiles": "open",
    "gitlens.codeLens.enabled": false,
    "update.enableWindowsBackgroundUpdates": false,
    "update.mode": "manual",
    "Lua.workspace.maxPreload": 99999999,
    "Lua.diagnostics.ignoredFiles": "Disable",
    "factorio.workspace.library": "c:\\FactorioModding\\runtime-api",
    "Lua.runtime.version": "Lua 5.2",
    "Lua.completion.workspaceWord": false,
    "extensions.autoUpdate": false,
    "Lua.runtime.builtin": {
        "math": "enable"
    },
    "Lua.diagnostics.neededFileStatus": {},
    "Lua.diagnostics.libraryFiles": "Disable",
    "Lua.diagnostics.groupFileStatus": {
        "ambiguity": "Any",
        "await": "Any",
        "codestyle": "None",
        "duplicate": "Any",
        "global": "Any",
        "luadoc": "Any",
        "redefined": "Any",
        "strict": "Any",
        "strong": "Any",
        "type-check": "Any",
        "unbalanced": "Any",
        "unused": "Any"
    },
    "Lua.diagnostics.groupSeverity": {},
    "Lua.misc.parameters": [
        "--develop=false",
        "--loglevel=warn"
    ],
    "Lua.hover.expandAlias": false,
    "Lua.type.castNumberToInteger": false,
    "Lua.semantic.keyword": true,
    "html.format.wrapLineLength": 0,
    "cSpell.enableFiletypes": [
        "lua",
        "factorio-locale",
        "factorio-changelog"
    ],
    "cSpell.customDictionaries": {
        "missing-words": {
            "name": "missing-words",
            "path": "../../.vscode/cspell/missing-words.txt",
            "addWords": false,
            "scope": "user"
        },
        "factorio-dictionary": {
            "name": "factorio-dictionary",
            "path": "../../.vscode/cspell/factorio-dictionary.txt",
            "addWords": false,
            "scope": "user"
        },
        "english-gb-us": {
            "name": "english-gb-us",
            "path": "../../.vscode/cspell/english-gb-us.txt",
            "addWords": false,
            "scope": "user"
        },
        "colors": {
            "name": "colors",
            "path": "../../.vscode/cspell/colors.txt",
            "addWords": false,
            "scope": "user"
        },
        "utility-library": {
            "name": "utility-library",
            "path": "../../.vscode/cspell/utility-library.txt",
            "addWords": false,
            "scope": "user"
        },
        "workspace-dictionary": {
            "name": "workspace-dictionary",
            "path": "./.vscode/cspell/workspace-dictionary.txt",
            "addWords": true,
            "scope": "user"
        }
    },
    "cSpell.spellCheckOnlyWorkspaceFiles": true,
    "cSpell.language": "en-GB",
    "cSpell.flagWords": [
        "armour",
        "colour",
        "centre",
        "behaviour"
    ],
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/objects",
        ".vscode",
        ".vscode-insiders",
        "**/settings.json",
        "**/token-styling",
        "**/textmate-colors"
    ],
    "editor.defaultFormatter": "sumneko.lua",
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "Lua.format.defaultConfig": {
        "max_line_length": "99999999"
    },
    "cSpell.suggestionMenuType": "quickFix",
    "cSpell.suggestionNumChanges": 5,
    "cSpell.spellCheckDelayMs": 1000,
    "cSpell.minWordLength": 2,
    "cSpell.logLevel": "Information",
    "Lua.hint.arrayIndex": "Enable",
    "cSpell.allowedSchemas": [
        "file",
        "gist",
        "sftp",
        "untitled",
        "vscode-notebook-cell",
        "vscode-scm",
        "vscode-userdata",
        "text"
    ],
    "window.newWindowDimensions": "offset",
}